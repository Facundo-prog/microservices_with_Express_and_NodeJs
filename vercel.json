{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "db/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/auth(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/user(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/post(.*)",
            "dest": "/api/index.js"
        }
    ]
}